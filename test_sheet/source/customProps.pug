include k-scaffold

+text({
  name:'attribute with custom props',
  trigger:{
    customNum:1,
    customString:'test',
    customArr:[1,2,3],
    customObj:{
      prop1:'val1',
      prop2:'val2'
    },
    nestedObj:{
      nest1:{
        nestedProp1:1,
        nestedArr: [1,2,3,4]
      },
      nest2:{
        nestedString: 'test',
        nestedObj:{
          num1:3,
          num2:4
        }
      }
    }
  }
})

+text({
  name:'attribute with custom props',
  trigger:{
    customNum:2,
    customString:'other test',
    customArr:[5,6],
    customObj:{
      prop2:'val other',
      prop3:'val3'
    },
    nestedObj:{
      nest1:{
        nestedProp1:2,
        nestedArr: [5]
      },
      nest3:{
        nestedString: 'test',
        nestedObj:{
          num3:3,
          num4:4
        }
      }
    }
  }
})

+kscript('custom prop test')